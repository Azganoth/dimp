{
	"extends": "../jsconfig",
	"compilerOptions": {
		"module": "ESNext",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"jsx": "react",
		"sourceMap": true,
		"outDir": "../build",
		"moduleResolution": "Node",
		"baseUrl": "../",
		"paths": {
			"app": ["app"]
		},
		"isolatedModules": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true
	},
	"exclude": []
}
