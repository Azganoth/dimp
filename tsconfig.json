{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": ["ES2020"],
		"allowJs": true,
		"jsx": "react",
		"sourceMap": true,
		"outDir": "public",
		"isolatedModules": true,
		"strict": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"exclude": ["app"]
}
